            $(document).ready(function () {
                var socket = io();
                setup($('#mainbody'), "#{user.username}", "#{user.displayname}", socket)
            });
    body.bodygrad
        ul
            li
                | Logged in as:
                strong #{user.displayname}
                br
                | Id:
                strong #{user.username}
        #mainbody(style='margin-bottom:20;margin-top:20;margin-left:20;margin-right:20; max-height:200px')
